echo "basedir:"${basedir}

cd ${basedir}/envs

echo "unset envs"
unset EASY_YAPI_PR_TITLE
unset EASY_YAPI_PR_BODY
echo "set envs"
echo "pwd:"`pwd`
echo "EASY_YAPI_PR_TITLE:"`cat EASY_YAPI_PR_TITLE`
export EASY_YAPI_PR_TITLE=`cat EASY_YAPI_PR_TITLE`
echo "EASY_YAPI_PR_BODY:"`cat EASY_YAPI_PR_BODY`
export EASY_YAPI_PR_BODY=`cat EASY_YAPI_PR_BODY`
echo "set envs completed"